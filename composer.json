{
    "name": "codemonster-ru/env",
    "description": "Lightweight .env loader for PHP projects",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Codemonster\\": "src/"
        },
        "files": [
            "helpers.php"
        ]
    },
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6 || ^10.5 || ^11.0 || ^12.0"
    },
    "scripts": {
        "test": "phpunit --colors=always"
    }
}
